{
	"parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"rules": {
		"strict": [2, "never"],
		"prefer-template": "error",
		"padded-blocks": "off",
		"comma-dangle": ["error", "never"],
		"no-console": "off",
		"guard-for-in": "off",
		"import/no-dynamic-require": "off",
		"global-require": "off"
	},
	"extends": [
		"airbnb"
	]
}
